{
  "extends": [
    "standard",
    "next/core-web-vitals"
  ],
  "rules": {
    "brace-style": [
      "error",
      "stroustrup"
    ],
    "curly": [
      "error",
      "multi"
    ],
    "camelcase": [
      "error",
      {
        "ignoreImports": true,
        "ignoreDestructuring": true,
        "allow": ["max_tokens", "database_id", "start_cursor", "page_id", "rich_text", "is_not_empty", "participant_email"]
      }
    ]
  }
}
